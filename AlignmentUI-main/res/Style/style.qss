/* ==================== QPushButton Styles (Responsive) ==================== */
/* Retain original styles for explicitly colored buttons */
QPushButton#but_start_auto {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #34C759, stop:1 #28A745);
    color: white;
    border-radius: 0.5em;
    padding: 0.4em;
    border: none;
    font-weight: bold;
    min-height: 1.5em;
    min-width: 4em;
}
QPushButton#but_start_auto:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #40D965, stop:1 #34C759);
}
QPushButton#but_start_auto:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #28A745, stop:1 #1E7B33);
}
QPushButton#but_start_auto:disabled {
    background: #4B5E67;
    color: #78909C;
}

QPushButton#but_start_camera_teaching:disabled {
    background: #D0D0D0;
    color: #666;
}

QPushButton#but_stop_auto {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FF3B30, stop:1 #D32F2F);
    color: white;
    border-radius: 0.5em;
    padding: 0.4em;
    border: none;
    font-weight: bold;
    min-height: 1.5em;
    min-width: 4em;
}
QPushButton#but_stop_auto:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FF5555, stop:1 #FF3B30);
}
QPushButton#but_stop_auto:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #D32F2F, stop:1 #B71C1C);
}
QPushButton#but_stop_auto:disabled {
    background: #4B5E67;
    color: #78909C;
}

QPushButton#but_reset_auto, QPushButton#but_save_teaching {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #2196F3, stop:1 #1976D2);
    color: white;
    border-radius: 0.5em;
    padding: 0.4em;
    border: none;
    font-weight: bold;
    min-height: 1.5em;
    min-width: 4em;
}
QPushButton#but_reset_auto:hover, QPushButton#but_save_teaching:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #42A5F5, stop:1 #2196F3);
}
QPushButton#but_reset_auto:pressed, QPushButton#but_save_teaching:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #1976D2, stop:1 #1565C0);
}

QPushButton#but_add {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #34C759, stop:1 #28A745);
    color: white;
    border-radius: 0.5em;
    padding: 0.4em;
    border: none;
    font-weight: bold;
    min-height: 1.5em;
    min-width: 4em;
}
QPushButton#but_add:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #40D965, stop:1 #34C759);
}
QPushButton#but_add:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #28A745, stop:1 #1E7B33);
}

QPushButton#but_del {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FF3B30, stop:1 #D32F2F);
    color: white;
    border-radius: 0.5em;
    padding: 0.4em;
    border: none;
    font-weight: bold;
    min-height: 1.5em;
    min-width: 4em;
}

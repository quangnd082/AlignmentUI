    border-radius: 0.4em;
    font-size: 2.7em;
    min-height: 1.2em;
}
QLabel[status="FAIL"] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FF3B30, stop:1 #D32F2F);
    color: white;
    font-weight: bold;
    padding: 0.3em;
    border-radius: 0.4em;
    font-size: 2.7em;
    min-height: 1.2em;
}


/* ==================== General QPushButton (Responsive Light Mode) ==================== */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    padding: 0.45em;
    color: #333333;
    font-weight: bold;
    min-height: 1.2em;
    min-width: 3em;
}
QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #D0D0D0, stop:1 #C0C0C0);
}

/* ==================== QMessageBox Buttons (Responsive) ==================== */
QMessageBox QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    border-radius: 0.36em;
    padding: 0.36em 1.8em;
    border: 0.05em solid #B0B0B0;
    min-height: 1.2em;
    min-width: 3em;
}
QMessageBox QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QMessageBox QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #D0D0D0, stop:1 #C0C0C0);
}

/* ==================== QTabWidget (Responsive) ==================== */
QTabWidget#tab_widget_main::pane {
    border: 0.05em solid #B0B0B0;
    border-radius: 0.55em;
    background: #F5F5F5;
    padding: 0.55em;
}

QTabWidget::pane {
    border: 0.05em solid #B0B0B0;
    border-radius: 0.55em;
    background: #F5F5F5;
    padding: 0.55em;
}

QTabBar::tab {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    border: 0.05em solid #B0B0B0;
    padding: 0.36em 0.9em;
    border-radius: 0.45em;
    margin: 0.18em;
    color: #333333;
    min-height: 1em;
    min-width: 2em;
}
QTabBar::tab:selected {
    background: #E0E0E0;
    border-bottom: 0.09em solid #A0A0A0;
    color: #333333;
}
QTabBar::tab:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}

/* ==================== QComboBox (Responsive) ==================== */
QComboBox {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    padding: 0.36em;
    border-radius: 0.45em;
    min-width: 2.7em;
    color: #333333;
    min-height: 1em;
}
QComboBox:hover {
    border-color: #A0A0A0;
}
QComboBox:on {
    background: #FFFFFF;
}
QComboBox QAbstractItemView {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    selection-background-color: #E0E0E0;
    selection-color: #333333;
    border-radius: 0.36em;
    color: #333333;
}
QComboBox QAbstractItemView::item:hover {
    background: #FFFFFF;
    color: #333333;
}
QComboBox QAbstractItemView::item:selected {
    background: #E0E0E0;
    color: #333333;
}
QComboBox::drop-down {
    border-radius: 0.36em;
    border: none;
    width: 1.8em;
    background: #bdb7b7;
}

/* ==================== QSpinBox & QDoubleSpinBox (Responsive) ==================== */
QSpinBox, QDoubleSpinBox {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    padding: 0.36em;
    color: #333333;
    min-height: 1em;
    min-width: 2em;
}
QSpinBox:hover, QDoubleSpinBox:hover {
    border-color: #A0A0A0;
}
QSpinBox:focus, QDoubleSpinBox:focus {
    border-color: #E0E0E0;
}

/* ==================== QLineEdit (Responsive) ==================== */
QLineEdit {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    padding: 0.36em;
    min-width: 2.7em;
    color: #333333;
    min-height: 1em;
}
QLineEdit:hover {
    border-color: #A0A0A0;
}
QLineEdit:focus {
    border-color: #E0E0E0;
}

/* ==================== QMenuBar (Responsive) ==================== */
QMenuBar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F5F5F5);
    padding: 0.18em 0.55em;
    border-bottom: 0.05em solid #D0D0D0;
}
QMenuBar::item {
    background: transparent;
    padding: 0.36em 0.73em;
    border-radius: 0.27em;
    color: #333333;
}
QMenuBar::item:hover {
    background: #E8ECEF;
    border: 0.05em solid #A0A0A0;
    color: #222222;
}
QMenuBar::item:pressed {
    background: #D0D0D0;
    color: #333333;
}
QMenu {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.36em;
}
QMenu::item {
    padding: 0.36em 1.09em;
    border-radius: 0.27em;
    color: #333333;
}
QMenu::item:selected {
    background: #D0D0D0;
    color: #333333;
}

/* ==================== QGroupBox (Responsive) ==================== */
QGroupBox {
    border: 0.09em solid #B0B0B0;
    border-radius: 0.45em;
    margin-top: 0.9em;
    font-weight: bold;
    color: #333333;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0.27em 0.73em;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    border: 0.05em solid #B0B0B0;
    border-radius: 0.36em;
    color: #333333;
}

/* ==================== QMainWindow & QWidget (Responsive Base) ==================== */
QMainWindow {
    background: #F5F5F5;
    color: #333333;
    font-family: "Segoe UI", "Arial", sans-serif;
    font-size: 1em; /* Base font size - will scale with system */
}
QWidget {
    color: #333333;
    font-size: 1em; /* Relative to parent */
}

/* ==================== QToolBox (Responsive) ==================== */
QToolBox, QToolBox QWidget {
    background: #F5F5F5;
}
QToolBox {
    border: 0.05em solid #B0B0B0;
    border-radius: 0.55em;
    padding: 0.45em;
}
QToolBox::tab {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    padding: 0.23em 0.68em;
    min-height: 1em;
}
QToolBox::tab:selected {
    background: #E0E0E0;
    color: #333333;
    border-bottom: 0.09em solid #A0A0A0;
}
QToolBox::tab:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}

/* ==================== QDateTimeEdit (Responsive) ==================== */

/* ==================== QLabel Status (Responsive) ==================== */
QLabel[status="WAIT"],
QLabel[status="WAIT_PRODUCT"],
QLabel[status="WAIT_TRIGGER"] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFE082, stop:1 #FFCA28);
    color: black;
    font-weight: bold;
    padding: 0.3em;
    border-radius: 0.4em;
    font-size: 2.7em;
    min-height: 1.2em;
}

QLabel[status="PASS"] {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #34C759, stop:1 #28A745);

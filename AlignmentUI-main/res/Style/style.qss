QCalendarWidget #qt_calendar_calendarview {
    background: #F5F5F5;
    color: #333333;
}
QCalendarWidget #qt_calendar_prevmonth,
QCalendarWidget #qt_calendar_nextmonth {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    min-width: 1.5em;
    min-height: 1.5em;
}
QCalendarWidget #qt_calendar_prevmonth:hover,
QCalendarWidget #qt_calendar_nextmonth:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QCalendarWidget #qt_calendar_prevmonth:pressed,
QCalendarWidget #qt_calendar_nextmonth:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #D0D0D0, stop:1 #C0C0C0);
}
QCalendarWidget #qt_calendar_monthbutton,
QCalendarWidget #qt_calendar_yearbutton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    min-height: 1.2em;
    min-width: 3em;
}
QCalendarWidget #qt_calendar_monthbutton:hover,
QCalendarWidget #qt_calendar_yearbutton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QCalendarWidget #qt_calendar_monthbutton:pressed,
QCalendarWidget #qt_calendar_yearbutton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #D0D0D0, stop:1 #C0C0C0);
}

/* ==================== QTableWidget (Responsive) ==================== */
QTableWidget {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    color: #333333;
    gridline-color: #B0B0B0;
}
QTableWidget::item {
    padding: 0.2em;
    min-height: 1em;
}
QTableWidget::item:selected {
    background: #E0E0E0;
    color: #333333;
}
QTableWidget QHeaderView::section {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    border: 0.05em solid #B0B0B0;
    padding: 0.18em;
    min-height: 1em;
}
QTableWidget QHeaderView::section:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QTableWidget QTableCornerButton::section {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    border: 0.05em solid #B0B0B0;
}
QTableWidget QTableCornerButton::section:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #F5F5F5, stop:1 #E0E0E0);
}
QTableWidget QScrollBar:vertical, QTableWidget QScrollBar:horizontal {
    background: #F5F5F5;
    border: none;
    width: 0.55em;
    height: 0.55em;
}
QTableWidget QScrollBar::handle:vertical, QTableWidget QScrollBar::handle:horizontal {
    background: #B0B0B0;
    border-radius: 0.27em;
    min-height: 0.9em;
    min-width: 0.9em;
}
QTableWidget QScrollBar::handle:vertical:hover, QTableWidget QScrollBar::handle:horizontal:hover {
    background: #A0A0A0;
}
QTableWidget QScrollBar::handle:vertical:pressed, QTableWidget QScrollBar::handle:horizontal:pressed {
    background: #E0E0E0;
}
QTableWidget QScrollBar::add-line:vertical, QTableWidget QScrollBar::sub-line:vertical,
QTableWidget QScrollBar::add-line:horizontal, QTableWidget QScrollBar::sub-line:horizontal {
    background: none;
    border: none;
}
QTableWidget QScrollBar::add-page:vertical, QTableWidget QScrollBar::sub-page:vertical,
QTableWidget QScrollBar::add-page:horizontal, QTableWidget QScrollBar::sub-page:horizontal {
    background: none;
}

/* ==================== QListView (Responsive) ==================== */
QListView {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.36em;
    color: #333333;
}
QListView::item {
    background: #F5F5F5;
    color: #333333;
    border: none;
    padding: 0.2em;
    min-height: 1em;
}
QListView::item:selected {
    background: #E0E0E0;
    color: #333333;
}
QListView QScrollBar:vertical, QListView QScrollBar:horizontal {
    background: #F5F5F5;
    border: none;
    width: 0.55em;
    height: 0.55em;
}
QListView QScrollBar::handle:vertical, QListView QScrollBar::handle:horizontal {
    background: #B0B0B0;
    border-radius: 0.27em;
    min-height: 0.9em;
    min-width: 0.9em;
}
QListView QScrollBar::handle:vertical:hover, QListView QScrollBar::handle:horizontal:hover {
    background: #A0A0A0;
}
QListView QScrollBar::handle:vertical:pressed, QListView QScrollBar::handle:horizontal:pressed {
    background: #E0E0E0;
}
QListView QScrollBar::add-line:vertical, QListView QScrollBar::sub-line:vertical,
QListView QScrollBar::add-line:horizontal, QListView QScrollBar::sub-line:horizontal {
    background: none;
    border: none;
}
QListView QScrollBar::add-page:vertical, QListView QScrollBar::sub-page:vertical,
QListView QScrollBar::add-page:horizontal, QListView QScrollBar::sub-page:horizontal {
    background: none;
}

/* ==================== Global Scrollbar Styles (Responsive) ==================== */
QScrollBar:vertical {
    background: #F5F5F5;
    width: 0.7em;
    border-radius: 0.35em;
    margin: 0;
}
QScrollBar::handle:vertical {
    background: #B0B0B0;
    border-radius: 0.35em;
    min-height: 1em;
    margin: 0.05em;
}
QScrollBar::handle:vertical:hover {
    background: #A0A0A0;
}
QScrollBar::handle:vertical:pressed {
    background: #909090;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
    border: none;
    height: 0;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    background: #F5F5F5;
    height: 0.7em;
    border-radius: 0.35em;
    margin: 0;
}
QScrollBar::handle:horizontal {
    background: #B0B0B0;
    border-radius: 0.35em;
    min-width: 1em;
    margin: 0.05em;
}
QScrollBar::handle:horizontal:hover {
    background: #A0A0A0;
}
QScrollBar::handle:horizontal:pressed {
    background: #909090;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    background: none;
    border: none;
    width: 0;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

/* ==================== QCheckBox & QRadioButton (Responsive) ==================== */
QCheckBox, QRadioButton {
    color: #333333;
    padding: 0.2em;
    spacing: 0.3em;
}
QCheckBox::indicator, QRadioButton::indicator {
    width: 1em;
    height: 1em;
    border: 0.05em solid #B0B0B0;
    background: #F5F5F5;
}
QCheckBox::indicator {
    border-radius: 0.2em;
}
QRadioButton::indicator {
    border-radius: 0.5em;
}
QCheckBox::indicator:checked {
    background: #2196F3;
    border-color: #1976D2;
}
QRadioButton::indicator:checked {
    background: #2196F3;
    border-color: #1976D2;
}
QCheckBox::indicator:hover, QRadioButton::indicator:hover {
    border-color: #A0A0A0;
}

/* ==================== QProgressBar (Responsive) ==================== */
QProgressBar {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    text-align: center;
    color: #333333;
    min-height: 1em;
}
QProgressBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #4CAF50, stop:1 #2E7D32);
    border-radius: 0.4em;
    margin: 0.05em;
}

/* ==================== QSlider (Responsive) ==================== */
QSlider::groove:horizontal {
    background: #E0E0E0;
    height: 0.3em;
    border-radius: 0.15em;
}
QSlider::handle:horizontal {
    background: #2196F3;
    border: 0.05em solid #1976D2;
    width: 1em;
    height: 1em;
    margin: -0.35em 0;
    border-radius: 0.5em;
}
QSlider::handle:horizontal:hover {
    background: #42A5F5;
}
QSlider::groove:vertical {
    background: #E0E0E0;
    width: 0.3em;
    border-radius: 0.15em;
}
QSlider::handle:vertical {
    background: #2196F3;
    border: 0.05em solid #1976D2;
    width: 1em;
    height: 1em;
    margin: 0 -0.35em;
    border-radius: 0.5em;
}
QSlider::handle:vertical:hover {
    background: #42A5F5;
}

/* ==================== QTextEdit & QPlainTextEdit (Responsive) ==================== */
QTextEdit, QPlainTextEdit {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    color: #333333;
    padding: 0.3em;
    selection-background-color: #E0E0E0;
    selection-color: #333333;
}
QTextEdit:hover, QPlainTextEdit:hover {
    border-color: #A0A0A0;
}
QTextEdit:focus, QPlainTextEdit:focus {
    border-color: #2196F3;
}

/* ==================== QStatusBar (Responsive) ==================== */
QStatusBar {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F5F5F5);
    border-top: 0.05em solid #D0D0D0;
    color: #333333;
    padding: 0.2em;
}
QStatusBar::item {
    border: none;
    padding: 0.1em;
}

/* ==================== QDockWidget (Responsive) ==================== */
QDockWidget {
    background: #F5F5F5;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.45em;
    color: #333333;
}
QDockWidget::title {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #FFFFFF, stop:1 #F0F0F0);
    color: #333333;
    padding: 0.3em;
    border-bottom: 0.05em solid #B0B0B0;
}
QDockWidget::close-button, QDockWidget::float-button {
    background: #E0E0E0;
    border: 0.05em solid #B0B0B0;
    border-radius: 0.2em;
    padding: 0.1em;
}
QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: #D0D0D0;
}

/* ==================== Responsive Font Size Classes ==================== */
/* These can be applied to specific widgets for different screen sizes */
.small-text {
    font-size: 0.8em;
}
.medium-text {
    font-size: 1em;
}
.large-text {
    font-size: 1.2em;
}
.extra-large-text {
    font-size: 1.5em;
}